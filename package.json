{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish:no-bump": "git checkout publish-to-npm && git merge --no-edit --no-ff main && git push && git checkout main",
    "publish:patch": "git checkout main && bump patch -t \"v%s\" -p && yarn publish:no-bump",
    "publish:minor": "git checkout main && bump minor -t \"v%s\" -p && yarn publish:no-bump",
    "publish:major": "git checkout main && bump major -t \"v%s\" -p && yarn publish:no-bump"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "name": "wow-combat-log-parser",
  "author": "Kun Chen",
  "module": "dist/wow-combat-log-parser.esm.js",
  "size-limit": [
    {
      "path": "dist/wow-combat-log-parser.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/wow-combat-log-parser.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/lodash": "^4.14.168",
    "husky": "^4.3.8",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "version-bump-prompt": "^6.1.0"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20",
    "moment": "^2.29.1"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  }
}
